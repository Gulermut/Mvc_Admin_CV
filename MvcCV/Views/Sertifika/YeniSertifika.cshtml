@model MvcCV.Models.Entity.TBLSERTİFİKALARIM
@{
    ViewBag.Title = "YeniSertifika";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}





<br />
<br />
<h4>Yeni Sertifika Ekleme Sayfası</h4>
<form method="post">

    @Html.Label("Sertifika Açıklaması")
    @Html.TextBoxFor(x => x.ACIKLAMA, new { @class = "form-control" })
    <br />
    @Html.Label(" Tarih")
    @Html.TextBoxFor(x => x.TARİH, new { @class = "form-control" })

    <br />
    <button class="btn btn-primary">Kaydet</button>
</form>





